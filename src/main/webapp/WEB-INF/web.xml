<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>TestServlet</servlet-name>
  	<display-name>TestServlet</display-name>
  	<description></description>
  	<servlet-class>com.tianli.TestServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DbServlet</servlet-name>
  	<display-name>DbServlet</display-name>
  	<description></description>
  	<servlet-class>com.tianli.DbServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DBSelectServlet</servlet-name>
  	<display-name>DBSelectServlet</display-name>
  	<description></description>
  	<servlet-class>com.tianli.DBSelectServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.tianli.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminServlet</servlet-name>
  	<display-name>AdminServlet</display-name>
  	<description></description>
  	<servlet-class>com.tianli.servlet.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>TestServlet</servlet-name>
  	<url-pattern>/testlink</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DbServlet</servlet-name>
  	<url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DBSelectServlet</servlet-name>
  	<url-pattern>/query</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AdminServlet</servlet-name>
  	<url-pattern>/admin</url-pattern>
  </servlet-mapping>
</web-app>
